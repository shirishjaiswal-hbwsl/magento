<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="urn:magento:framework:App/etc/routes.xsd">
    <body>
        <referenceBlock name="head.additional">
            <block name="header_footer_script_default"
                template="Shirish_HeaderFooterScript::head_additional/default.phtml"
                class="Magento\Framework\View\Element\Template" />
        </referenceBlock>
        <referenceBlock name="after.body.start">
            <block name="after_body_start_default_script"
                template="Shirish_HeaderFooterScript::body_start/default_script.phtml"
                class="Magento\Framework\View\Element\Template" />
        </referenceBlock>
        <referenceBlock name="before.body.end">
            <block name="before_body_end_default_script"
                template="Shirish_HeaderFooterScript::body_end/default_script.phtml"
                class="Magento\Framework\View\Element\Template" />
        </referenceBlock>
    </body>
</page>