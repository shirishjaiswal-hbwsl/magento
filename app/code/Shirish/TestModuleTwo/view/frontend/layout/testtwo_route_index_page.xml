<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="urn:magento:framework:App/etc/routes.xsd">
    <head>
        <title>
            This is Head Layout
        </title>
    </head>
    <body>
        <!-- 
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument name="page_title" xsi:type="string">Something</argument>
            </action>
        </referenceBlock> -->
        <!-- <referenceBlock name="page.main.title" remove="true" /> -->
        <!--this
        referenceContainer name attribute is refering the name to the container present in the
        Magento\Framework\View\Element\Template"  -->
        <referenceContainer name="content">
            <!-- class path should have facet pattern i.e., first letter of each dir should be
            capital -->
            <!-- Now we are using the custom block which we have created rather then we generic one -->
            <block name="layout_example"
                class="Shirish\TestModuleTwo\Block\Layout\Index"
                template="Shirish_TestModuleTwo::content.phtml"
            >
                <block name="layout_note" class="Shirish\TestModuleTwo\Block\Layout\Note"
                    template="Shirish_TestModuleTwo::note.phtml" />

            </block>
            <block name="cms_block_element" class="Magento\Cms\Block\Block">
                <arguments>
                    <argument name="block_id" xsi:type="string">cms_block_element</argument>
                </arguments>
            </block>

        </referenceContainer>
    </body>
</page>